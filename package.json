{
  "name": "shr-client-ts",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:type",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --project ./tsconfig.compile.json",
    "build:esm": "tsc --module es2015 --outDir dist/esm --project ./tsconfig.compile.json",
    "build:type": "tsc -d --emitDeclarationOnly --outDir dist/types --project ./tsconfig.compile.json",
    "build:umd": "webpack --config webpack.config.js",
    "clean": "rm -rf dist/",
    "test": "yarn test:unit && yarn test:coverage",
    "test:unit": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' env NODE_ENV=test mocha",
    "test:coverage": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' env NODE_ENV=test nyc mocha",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build"
  },
  "files": [
    "/dist",
    "/scripts"
  ],
  "dependencies": {
    "@confio/ics23": "^0.6.8",
    "@cosmjs/amino": "0.27.1",
    "@cosmjs/crypto": "0.27.1",
    "@cosmjs/encoding": "0.27.1",
    "@cosmjs/math": "0.27.1",
    "@cosmjs/stream": "0.27.1",
    "@cosmjs/tendermint-rpc": "0.27.1",
    "@cosmjs/utils": "0.27.1",
    "bignumber.js": "^9.0.2",
    "long": "^5.2.0",
    "protobufjs": "^6.11.2",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/long": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-bytes": "^0.1.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "ts-proto": "^1.93.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "repository": "https://bitbucket.org/shareringvietnam/shr-client-ts",
  "author": "Tan Do (Alex) <tan.do@sharering.network>",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
