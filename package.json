{
  "name": "shr-cosmos-client",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:type",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --project ./tsconfig.compile.json",
    "build:esm": "tsc --module es2015 --outDir dist/esm --project ./tsconfig.compile.json",
    "build:type": "tsc -d --emitDeclarationOnly --outDir dist/types --project ./tsconfig.compile.json",
    "build:umd": "rollup -c",
    "clean": "rm -rf dist/",
    "test": "yarn test:unit && yarn test:coverage",
    "test:unit": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' env NODE_ENV=test mocha",
    "test:coverage": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' env NODE_ENV=test nyc mocha",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write --loglevel warn"
  },
  "files": [
    "/dist",
    "/scripts"
  ],
  "dependencies": {
    "@confio/ics23": "^0.6.5",
    "@cosmjs/amino": "^0.25.4",
    "@cosmjs/encoding": "^0.25.4",
    "@cosmjs/math": "^0.25.4",
    "@cosmjs/stream": "^0.25.4",
    "@cosmjs/tendermint-rpc": "^0.25.4",
    "@cosmjs/utils": "^0.25.4",
    "long": "^4.0.0",
    "protobufjs": "^6.11.2",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@cosmjs/crypto": "^0.25.4",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/long": "^4.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-bytes": "^0.1.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.81.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "repository": "https://bitbucket.org/shareringvietnam/shr-cosmos-client",
  "author": "Tan Do (Alex) <tan.do@sharering.network>",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
